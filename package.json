    "@neutrinojs/jest": "9.x",
    "@neutrinojs/library": "9.x",
    "@types/jest": "26.x",
    "babel-plugin-transform-dynamic-import": "2.x",
    "eslint": "7.x",
    "ioredis": "4.x",
    "jest": "26.x",
    "mongodb": "3.x",
    "neutrino": "9.x",
    "neutrinojs-typescript": "1.x",
    "neutrinojs-typescript-eslint": "1.x",
