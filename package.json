{
  "name": "collection-storage",
  "version": "1.5.0",
  "description": "abstraction layer around communication with a collection-based database",
  "files": [
    "build/**/*"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "webpack --mode production && tsc -p tsconfig-decl.json",
    "lint": "eslint --format codeframe --ext mjs,jsx,js,tsx,ts --report-unused-disable-directives src && tsc",
    "test": "jest && npm run lint",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidje13/collection-storage.git"
  },
  "keywords": [
    "nosql",
    "collection",
    "persistence",
    "database",
    "mongo",
    "in-memory"
  ],
  "author": "David Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davidje13/collection-storage/issues"
  },
  "homepage": "https://github.com/davidje13/collection-storage#readme",
  "dependencies": {
    "@types/mongodb": "^3.3.1"
  },
  "devDependencies": {
    "@neutrinojs/airbnb-base": "^9.0.0-rc.3",
    "@neutrinojs/jest": "^9.0.0-rc.3",
    "@neutrinojs/library": "^9.0.0-rc.3",
    "@types/jest": "^24.0.18",
    "babel-plugin-transform-dynamic-import": "^2.1.0",
    "eslint": "^5.16.0",
    "jest": "^24.9.0",
    "mongodb": "^3.3.2",
    "neutrino": "^9.0.0-rc.3",
    "neutrino-typescript": "git+https://github.com/davidje13/neutrino-typescript#semver:^1.0.2",
    "neutrino-typescript-eslint": "git+https://github.com/davidje13/neutrino-typescript-eslint#semver:^1.0.0",
    "typescript": "^3.6.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  }
}
